                        </div>
                    </div>

                    <!--end::Widget -->
                </div>
            </div>

            <!--end:: Widgets/Applications/User/Profile1-->
        </div>

        <!--End:: App Aside-->
        <!--Begin:: App Content-->
        <div class="kt-grid__item kt-grid__item--fluid kt-app__content">
            <div class="row">
                <div class="col-xl-12">
                    <div class="kt-portlet">
                        <div class="kt-portlet__head">
                            <div class="kt-portlet__head-label">
                                <h3 class="kt-portlet__head-title">账户信息 <small>更改您的账户信息</small></h3>
                            </div>
                            <div class="kt-portlet__head-toolbar">
                                <div class="kt-portlet__head-wrapper">
                                    <div class="dropdown dropdown-inline">
                                        <button type="button" class="btn btn-label-brand btn-sm btn-icon btn-icon-md" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="flaticon2-console"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-right">
                                            <ul class="kt-nav">
                                                <li class="kt-nav__section kt-nav__section--first">
                                                    <span class="kt-nav__section-text">Export Tools</span>
                                                </li>
                                                <li class="kt-nav__item">
                                                    <a href="#" class="kt-nav__link">
                                                        <i class="kt-nav__link-icon la la-print"></i>
                                                        <span class="kt-nav__link-text">Print</span>
                                                    </a>
                                                </li>
                                                <li class="kt-nav__item">
                                                    <a href="#" class="kt-nav__link">
                                                        <i class="kt-nav__link-icon la la-copy"></i>
                                                        <span class="kt-nav__link-text">Copy</span>
                                                    </a>
                                                </li>
                                                <li class="kt-nav__item">
                                                    <a href="#" class="kt-nav__link">
                                                        <i class="kt-nav__link-icon la la-file-excel-o"></i>
                                                        <span class="kt-nav__link-text">Excel</span>
                                                    </a>
                                                </li>
                                                <li class="kt-nav__item">
                                                    <a href="#" class="kt-nav__link">
                                                        <i class="kt-nav__link-icon la la-file-text-o"></i>
                                                        <span class="kt-nav__link-text">CSV</span>
                                                    </a>
                                                </li>
                                                <li class="kt-nav__item">
                                                    <a href="#" class="kt-nav__link">
                                                        <i class="kt-nav__link-icon la la-file-pdf-o"></i>
                                                        <span class="kt-nav__link-text">PDF</span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <form asp-controller="Personal" asp-action="Update" id="user_edit_form" class="kt-form kt-form--label-right" method="post">
                            <div class="kt-portlet__body">
                                <div class="kt-section kt-section--first">
                                    <div class="kt-section__body">
                                        <div class="row">
                                            <label class="col-xl-3"></label>
                                            <div class="col-lg-9 col-xl-6">
                                                <h3 class="kt-section__title kt-section__title-sm">个人信息:</h3>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">真实姓名*</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <input asp-for="FullName" name="fullname" class="form-control" type="text">
                                            </div>
                                        </div>
                                        @if (User.IsInRole(Roles.Shop))
                                        {
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">网址</label>
                                                <div class="col-lg-9 col-xl-6">
                                                    <input asp-for="SiteAddress" name="siteaddress" class="form-control" type="text">
                                                    <span class="form-text text-muted">商户的网页连结。</span>
                                                </div>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">昵称</label>
                                                <div class="col-lg-9 col-xl-6">
                                                    <input asp-for="Nickname" name="nickname" class="form-control" type="text">
                                                    <span class="form-text text-muted">可留白并使用真实姓名。</span>
                                                </div>
                                            </div>
                                        }
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">手机号码*</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <div class="input-group">
                                                    <div class="input-group-prepend"><span class="input-group-text"><i class="la la-phone"></i></span></div>
                                                    <input asp-for="PhoneNumber" name="phonenumber" type="text" class="form-control" placeholder="Phone" aria-describedby="basic-addon1">
                                                </div>
                                                <span class="form-text text-muted">We'll never share your email with anyone else.</span>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">电子邮件*</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <div class="input-group">
                                                    <div class="input-group-prepend"><span class="input-group-text"><i class="la la-at"></i></span></div>
                                                    <input asp-for="Email" name="email" type="text" class="form-control" placeholder="manager@gmail.com" aria-describedby="basic-addon1">

                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">微信</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <input asp-for="Wechat" name="wechat" class="form-control" type="text">
                                            </div>
                                        </div>
                                        <div class="form-group form-group-last row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">QQ</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <div class="input-group">
                                                    <input asp-for="QQ" name="qq" class="form-control" type="text">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="kt-separator kt-separator--border-dashed kt-separator--portlet-fit kt-separator--space-lg"></div>
                                <div class="kt-section kt-section--first">
                                    <div class="kt-section__body">
                                        <div class="row">
                                            <label class="col-xl-3"></label>
                                            <div class="col-lg-9 col-xl-6">
                                                <h3 class="kt-section__title kt-section__title-sm">帐户细节:</h3>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">用户ID</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <input asp-for="UserId" class="form-control" type="text" value="@Model.UserId" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">用户名(登入账号)</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <input asp-for="UserName" class="form-control" type="text" value="@Model.UserName" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">上级代理</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <input asp-for="UplineId" class="form-control" type="text" value="@Model.UplineId" readonly>
                                            </div>
                                        </div>
                                        @if (User.IsInRole(Roles.TraderAgent) || User.IsInRole(Roles.ShopAgent))
                                        {
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">允许建立下级代理</label>
                                                <div class="col-lg-9 col-xl-6">
                                                    <span class="kt-switch kt-switch--outline kt-switch--icon kt-switch--success">
                                                        <label>
                                                            <input asp-for="HasGrantRight" type="checkbox" disabled>
                                                            <span></span>
                                                        </label>
                                                    </span>
                                                </div>
                                            </div>
                                        }
                                        <div class="form-group row">
                                            <label class="col-xl-3 col-lg-3 col-form-label">帐户状态</label>
                                            <div class="col-lg-9 col-xl-6">
                                                <span class="kt-switch kt-switch--outline kt-switch--icon kt-switch--success">
                                                    <label>
                                                        <input asp-for="IsEnabled" type="checkbox" disabled>
                                                        <span></span>
                                                    </label>
                                                </span>
                                            </div>
                                        </div>
