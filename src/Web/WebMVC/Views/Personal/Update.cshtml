@model WebMVC.ViewModels.PersonalViewModels.UpdateViewModel
@using WebMVC.Models.Roles

@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";

    ViewData["UserRole"] = string.Empty;
    if (User.IsInRole(Roles.Shop))
    {
        ViewData["UserRole"] = Roles.Shop;
    }
    else if (User.IsInRole(Roles.ShopAgent))
    {
        ViewData["UserRole"] = Roles.ShopAgent;
    }
    else if (User.IsInRole(Roles.Trader))
    {
        ViewData["UserRole"] = Roles.Trader;
    }
    else if (User.IsInRole(Roles.TraderAgent))
    {
        ViewData["UserRole"] = Roles.TraderAgent;
    }
}

