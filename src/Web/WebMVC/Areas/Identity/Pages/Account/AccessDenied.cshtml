@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<!--
<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">You do not have access to this resource.</p>
</header>
-->


<!-- begin::Body -->
<body class="kt-quick-panel--right kt-demo-panel--right kt-offcanvas-panel--right kt-header--fixed kt-header-mobile--fixed kt-subheader--enabled kt-subheader--solid kt-aside--enabled kt-aside--fixed kt-aside--minimize kt-page--loading">

    <!-- begin:: Page -->
    <div class="kt-grid kt-grid--ver kt-grid--root kt-page">
        <div class="kt-grid__item kt-grid__item--fluid kt-grid  kt-error-v1 kt-bg-light-light">
            <div class="kt-error-v1__container">
                <h1 class="kt-error-v1__number">403</h1>
                <h1 class="kt-error-v1__desc text-danger">@ViewData["Title"]</h1>
                <p class="kt-error-v1__desc kt-font-danger">
                    You do not have access to this resource.
                </p>
            </div>
        </div>
    </div>

    <!-- end:: Page -->
    <!-- begin::Global Config(global config for global JS sciprts) -->
    <script>
    var KTAppOptions = {
            "colors": {
                "state": {
                    "brand": "#22b9ff",
                    "light": "#ffffff",
                    "dark": "#282a3c",
                    "primary": "#5867dd",
                    "success": "#34bfa3",
                    "info": "#36a3f7",
                    "warning": "#ffb822",
                    "danger": "#fd3995"
                },
                "base": {
                    "label": ["#c5cbe3", "#a1a8c3", "#3d4465", "#3e4466"],
                    "shape": ["#f0f3ff", "#d9dffa", "#afb4d4", "#646c9a"]
                }
            }
        };</script>
</body>



@section AddToHead{
    <link href="~/lib/metronic/dist/assets/css/pages/error/error-1.css" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" href="~/lib/metronic/dist/assets/media/logos/favicon.ico" />
}